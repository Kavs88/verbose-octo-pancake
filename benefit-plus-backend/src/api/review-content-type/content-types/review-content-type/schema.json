{
  "kind": "collectionType",
  "collectionName": "review_content_types",
  "info": {
    "singularName": "review-content-type",
    "pluralName": "review-content-types",
    "displayName": "Review Content Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rating": {
      "type": "integer",
      "min": 0,
      "max": 5
    },
    "comment": {
      "type": "text"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "business": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::business.business",
      "inversedBy": "review_content_types"
    },
    "isVerified": {
      "type": "boolean",
      "default": false
    },
    "categories": {
      "type": "json"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
